        "name": "Login",
        "event": [
          {
            "listen": "test",
            "script": {
              "id": "4754b295-0380-4e59-abd8-90c3e8aeb454",
              "exec": [
                "// Extract accessToken and refreshToken from response and set as global variables",
                "if (pm.response.code === 200) {",
                "    var jsonData = pm.response.json();",
                "    if (jsonData.tokens && jsonData.tokens.accessToken && jsonData.tokens.refreshToken) {",
                "        pm.globals.set(\"accessToken\", jsonData.tokens.accessToken);",
                "        pm.globals.set(\"refreshToken\", jsonData.tokens.refreshToken);",
                "    }",
                "}"
              ],
              "type": "text/javascript",
              "packages": {},
              "requests": {}
            }
